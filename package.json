{
    "name": "2-forum",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write --ignore-unknown .",
        "start": "dotenv -- nest start",
        "start:dev": "dotenv -- nest start --watch",
        "start:debug": "dotenv -- nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint **/!(*.d).{js,ts} --fix",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:cov": "vitest run --coverage",
        "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --no-file-parallelism",
        "test:e2e": "vitest run --config ./test/vitest.config.ts",
        "test:e2e:watch": "vitest --config ./test/vitest.config.ts",
        "prepare": "husky",
        "typeorm": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js --dataSource ./src/config/database.ts",
        "migration:generate": "npm run build && npm run typeorm -- migration:generate ./migrations/%npm_config_name%",
        "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create ./migrations/%npm_config_name%",
        "migration:run": "npm run build && npm run typeorm -- migration:run",
        "migration:revert": "npm run typeorm -- migration:revert"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.11.2",
        "@nestjs/bull": "^10.1.1",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.2",
        "@nestjs/core": "^10.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/platform-fastify": "^10.3.7",
        "@nestjs/swagger": "^7.3.1",
        "@nestjs/throttler": "^5.1.2",
        "@nestjs/typeorm": "^10.0.2",
        "bull": "^4.12.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "google-auth-library": "^9.8.0",
        "handlebars": "^4.7.8",
        "joi": "^17.12.3",
        "lodash.clone": "^4.5.0",
        "lodash.isobject": "^3.0.2",
        "nestjs-throttler-storage-redis": "^0.4.4",
        "nodemailer": "^6.9.13",
        "pg": "^8.11.5",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.20",
        "typeorm-naming-strategies": "^4.1.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@fastify/static": "^7.0.3",
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@swc/core": "^1.4.14",
        "@types/express": "^4.17.17",
        "@types/imapflow": "^1.0.18",
        "@types/lodash.clone": "^4.5.9",
        "@types/lodash.isobject": "^3.0.9",
        "@types/node": "^20.3.1",
        "@types/nodemailer": "^6.4.14",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.1",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "^9.0.11",
        "imapflow": "^1.0.159",
        "lint-staged": "^15.2.2",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3",
        "unplugin-swc": "^1.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.5.0"
    }
